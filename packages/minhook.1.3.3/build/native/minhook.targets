    <Copy Condition="$(MH_LibSuffix) == 'x64-v100-md'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x64-v100-md.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x64-v100-mtd'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x64-v100-mtd.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x64-v100-mt'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x64-v100-mt.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x86-v110-mdd'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x86-v110-mdd.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x86-v110-md'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x86-v110-md.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
