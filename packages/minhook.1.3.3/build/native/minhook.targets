    <Copy Condition="$(MH_LibSuffix) == 'x64-v110-mt'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x64-v110-mt.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x86-v120-mdd'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x86-v120-mdd.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
    <Copy Condition="$(MH_LibSuffix) == 'x86-v120-md'" SourceFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook-x86-v120-md.lib" DestinationFiles="$(MSBuildThisFileDirectory)..\..\lib\native\lib\libMinHook.lib" />
